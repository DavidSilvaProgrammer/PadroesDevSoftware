package PadraoAdapter;

public class Iphone implements ConectorApple{
  private boolean conector;
}
  @override
  public void useConectorApple(){
    conector = true;
    System.out.println("Carregador da Apple plugado!");
  }
  @override
  public void recarrega(){
    if(conector){
      System.out.println("Come√ßou a recarregar!");
      System.out.println("Recarga finalizada!");
  }else{
    System.out.println("Conecte o carregador da Apple!")
  }
 }
}
